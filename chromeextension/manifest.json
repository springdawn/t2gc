{
    "name":"Twitter2GoogleCalendar",
    "version":"0.1",
    "manifest_version":2,

    "permissions":[
        "contextMenus",
        "http://localhost/",
        "tabs",
        "notifications"
    ],
    "background":{
        "persistent":false,
        "scripts":["js/jquery-2.0.3.min.js","js/t2gc.js"]
    },
    "browser_action":{
        "default_title":"T2G",
        "default_popup":"html/popup.html"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["js/content.js"]
        }
    ]
}

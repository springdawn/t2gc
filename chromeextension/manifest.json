{
    "name":"Twitter2GoogleCalendar",
    "version":"0.1",
    "manifest_version":2,

    "permissions":[
        "contextMenus",
        "http://localhost/",
        "tabs"
    ],
    "background":{
        "persistent":false,
        "scripts":["jquery-2.0.3.min.js","t2gc.js"]
    },
    "browser_action":{
        "default_title":"T2G",
        "default_popup":"popup.html"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ]
}
